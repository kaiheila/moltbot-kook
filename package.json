{
  "name": "@kookapp/moltbot-kook",
  "version": "1.0.2",
  "description": "Kook channel plugin for moltbot / Clawdbot / OpenClaw",
  "type": "module",
  "main": "index.ts",
  "exports": {
    ".": {
      "import": "./index.ts"
    }
  },
  "clawdbot": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "moltbot-kook",
      "label": "Kook",
      "selectionLabel": "Kook",
      "docsPath": "/channels/kook",
      "docsLabel": "kook",
      "blurb": "Kook chat platform integration.",
      "order": 80
    },
    "install": {
      "npmSpec": "@kookapp/moltbot-kook",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "files": [
    "index.ts",
    "src",
    "clawdbot.plugin.json"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "clawdbot",
    "moltbot",
    "kook",
    "chat",
    "bot",
    "plugin"
  ],
  "author": "Xiaolong Dong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clawdbot/kook.git"
  },
  "bugs": {
    "url": "https://github.com/clawdbot/kook/issues"
  },
  "homepage": "https://github.com/clawdbot/kook",
  "peerDependencies": {
    "clawdbot": ">=2026.1.0"
  },
  "dependencies": {
    "ws": "^8.19.0",
    "zod": "^3.24.1"
  }
}
